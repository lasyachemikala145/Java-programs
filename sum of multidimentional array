package demo;

public class MultiSum{
	public static void main(String [] args) {
		int[][] arr= {
				{1,2,3},
				{2,3,4},
		};
		int sum=0;
		for(int i=0;i<arr.length;i++) {//length -> for arrays we don't use paranthesis
			for(int j=0;j<arr[i].length;j++) {
				sum+=arr[i][j];
			
			}
		}
		System.out.println(sum);
	}

}
